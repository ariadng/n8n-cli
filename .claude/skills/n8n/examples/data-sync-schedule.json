{
  "name": "Scheduled Data Sync",
  "nodes": [
    {
      "id": "11111111-1111-1111-1111-111111111111",
      "name": "Every Hour",
      "type": "n8n-nodes-base.scheduleTrigger",
      "position": [250, 300],
      "parameters": {
        "rule": {
          "interval": [
            {
              "field": "hours",
              "hoursInterval": 1
            }
          ]
        }
      },
      "typeVersion": 1
    },
    {
      "id": "22222222-2222-2222-2222-222222222222",
      "name": "Fetch Source Data",
      "type": "n8n-nodes-base.httpRequest",
      "position": [450, 300],
      "parameters": {
        "url": "https://api.source.com/data",
        "method": "GET",
        "authentication": "genericCredentialType",
        "genericAuthType": "httpHeaderAuth"
      },
      "typeVersion": 4,
      "credentials": {
        "httpHeaderAuth": {
          "id": "1",
          "name": "Source API Key"
        }
      }
    },
    {
      "id": "33333333-3333-3333-3333-333333333333",
      "name": "Transform Data",
      "type": "n8n-nodes-base.code",
      "position": [650, 300],
      "parameters": {
        "jsCode": "const items = $input.all();\n\nreturn items.map(item => ({\n  json: {\n    id: item.json.id,\n    name: item.json.name,\n    syncedAt: new Date().toISOString(),\n    source: 'api'\n  }\n}));"
      },
      "typeVersion": 2
    },
    {
      "id": "44444444-4444-4444-4444-444444444444",
      "name": "Check Has Data",
      "type": "n8n-nodes-base.if",
      "position": [850, 300],
      "parameters": {
        "conditions": {
          "boolean": [
            {
              "value1": "={{ $json.id }}",
              "operation": "isNotEmpty"
            }
          ]
        }
      },
      "typeVersion": 1
    },
    {
      "id": "55555555-5555-5555-5555-555555555555",
      "name": "Upsert to Database",
      "type": "n8n-nodes-base.postgres",
      "position": [1050, 200],
      "parameters": {
        "operation": "upsert",
        "table": "synced_data",
        "columns": "id, name, synced_at, source",
        "conflictColumns": "id"
      },
      "typeVersion": 2,
      "credentials": {
        "postgres": {
          "id": "2",
          "name": "Production DB"
        }
      }
    },
    {
      "id": "66666666-6666-6666-6666-666666666666",
      "name": "Log Empty Result",
      "type": "n8n-nodes-base.code",
      "position": [1050, 400],
      "parameters": {
        "jsCode": "console.log('No data to sync');\nreturn [{ json: { status: 'no_data' } }];"
      },
      "typeVersion": 2
    }
  ],
  "connections": {
    "Every Hour": {
      "main": [
        [
          { "node": "Fetch Source Data", "type": "main", "index": 0 }
        ]
      ]
    },
    "Fetch Source Data": {
      "main": [
        [
          { "node": "Transform Data", "type": "main", "index": 0 }
        ]
      ]
    },
    "Transform Data": {
      "main": [
        [
          { "node": "Check Has Data", "type": "main", "index": 0 }
        ]
      ]
    },
    "Check Has Data": {
      "main": [
        [
          { "node": "Upsert to Database", "type": "main", "index": 0 }
        ],
        [
          { "node": "Log Empty Result", "type": "main", "index": 0 }
        ]
      ]
    }
  },
  "settings": {
    "saveExecutionProgress": true,
    "saveDataErrorExecution": "all",
    "saveDataSuccessExecution": "all",
    "executionTimeout": 300
  }
}
